command	nlsh.py	/^    command = result.choices[0]['text'].strip()$/;"	v
ogprompt	nlsh.py	/^ogprompt = """$/;"	v
prompt	nlsh.py	/^    prompt = ogprompt.rstrip() + template.format(request)$/;"	v
prompt	nlsh.py	/^    prompt = prompt.strip() + " " + command + "\\n###\\n"$/;"	v
request	nlsh.py	/^    request = input(click.style('nlsh> ', 'red', bold=True))$/;"	v
result	nlsh.py	/^    result = openai.Completion.create($/;"	v
template	nlsh.py	/^template = """$/;"	v
